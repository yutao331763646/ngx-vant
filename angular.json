{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ngx-vant": {
            "projectType": "library",
            "root": "projects/ngx-vant",
            "sourceRoot": "projects/ngx-vant/src",
            "prefix": "van",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/ngx-vant/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/ngx-vant/tsconfig.lib.prod.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ngx-vant/src/test.ts",
                        "tsConfig": "projects/ngx-vant/tsconfig.spec.json",
                        "karmaConfig": "projects/ngx-vant/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/ngx-vant/tsconfig.lib.json",
                            "projects/ngx-vant/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "vant-for-angular",
    "cli": {
        "analytics": "ea2b2145-f880-448c-9ae8-555d360368f4"
    }
}